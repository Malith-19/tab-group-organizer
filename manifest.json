{
  "manifest_version": 3,
  "name": "Tab Group Organizer",
  "version": "1.0",
  "description": "Automatically group tabs based on URL patterns.",
  "permissions": ["tabs", "tabGroups", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["<all_urls>"]
}
